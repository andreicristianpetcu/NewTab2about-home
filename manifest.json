{
    "name": "__MSG_appName__",
    "description": "__MSG_appDescription__",
    "author": "Andrei Petcu",
    "version": "1.2.0",
    "manifest_version": 2,
    "icons": {
      "32": "images/icon32x32.png",
      "64": "images/icon64x64.png"
    },
    "homepage_url": "https://github.com/andreicristianpetcu/NewTab2about-home",
    "chrome_url_overrides": {
